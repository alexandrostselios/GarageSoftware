<script type="text/javascript">
    console.log(11);
    $(function () {
        var importCarManufacturersDIVElement = $('#importCarManufacturers');
        console.log(33);
        $('button[data-bs-toggle="ajax-modal"]').click(function (event) {
            //console.log(44);
            var url = $(this).data('url');
            $.get(url).done(function (data) {
                //console.log(55);
                importCarManufacturersDIVElement.html(data);
                importCarManufacturersDIVElement.find('.modal').modal('show');
            })
        })

        //Save Add Makeform data
        importCarManufacturersDIVElement.on('click', '[data-bs-save="modal"]', function (event) {

            var form = $(this).parents('.modal').find('form');
            var actionUrl = form.attr("action");
            var sendData = form.serialize();
            $.post(actionUrl, sendData).done(function (data) {
                importCarManufacturersDIVElement.find('.modal').modal('hide');
                location.reload(true);
            })
        })
    })

</script>